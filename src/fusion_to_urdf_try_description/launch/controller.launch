<launch>

<rosparam file="$(find fusion_to_urdf_try_description)/launch/controller.yaml" command="load"/>
<node args="right_wheel_joint_position_controller left_whel_joint_position_controller lidar_joint_position_controller rgbd_joint_position_controller joint_state_controller " name="controller_spawner" ns="fusion_to_urdf_try" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/fusion_to_urdf_try/joint_states"/>
</node>

</launch>